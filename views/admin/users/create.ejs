      <div class="card mt-5">
        <div class="card-body">
          <h2>Criação de usuário</h2>
          <form action="/users/create" method="POST" class="mt-5">
            <input class="form-control mb-3" type="email" name="email" placeholder="email@exemplo.com">
            <input class="form-control mb-3" type="password" name="password" placeholder="senha">
            <button type="submit" class="btn btn-dark mt-3">Criar</button>
          </form>
        </div>
      </div>
    </main>
